{
  "disabled": false,
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "route": "costfromcosmos/{location}/{skuname}",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "name": "documents",
      "type": "cosmosDB",
      "direction": "in",
      "databaseName": "Configurator",
      "collectionName": "Skus",
      "sqlQuery": "SELECT * from c where c.name = {skuname} AND c.location = {location}",
      "connectionStringSetting": "CosmosDBConnection"
  },
    {
      "authLevel": null,
      "type": "http",
      "direction": "out",
      "name": "res",
      "methods": null
    }
  ],
  "scriptFile": "../dist/GetCost/index.js"
}